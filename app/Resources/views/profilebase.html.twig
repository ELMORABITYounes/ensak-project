<script>
    $(function() {
        $('.multipleSelect').select2();
            $(".close").click(function(e){
                e.preventDefault();
                $(this).closest(".alert-message").fadeOut(function(event){
                    $(this).remove();
                });
            });

        $("#toggle-menu").click(function () {
            var $lefty = $(".sidebar");
            if($(this).hasClass("hide")) {
                $(this).toggleClass("hide");
                $lefty.animate({left: parseInt($lefty.css('left'), 10) == 0 ? -$lefty.outerWidth()-20 : 0});
                $( ".theContent" ).animate({
                    marginLeft: "280px",
                } );
                $(".footer" ).animate({
                    marginLeft: "280px",
                } , function() {
                    $($.fn.dataTable.tables(true)).DataTable()
                        .columns.adjust()
                        .responsive.recalc();
                });

            }else {
                $(this).toggleClass("hide");
                $lefty.animate({left: parseInt($lefty.css('left'), 10) == 0 ? -$lefty.outerWidth()-20 : 0});
                $( ".theContent" ).animate({
                    marginLeft: "16px",
                });
                $( ".footer" ).animate({
                    marginLeft: "16px",
                }, function() {
                    $($.fn.dataTable.tables(true)).DataTable()
                        .columns.adjust()
                        .responsive.recalc();
                });
            }
        });
    });
</script>
